<mvc:View controllerName="DemoPatient.DemoPatient.controller.Progress" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<Toolbar>
						<ToolbarSpacer width="5%"/>
						<Button width="15%" icon="sap-icon://menu2"/>
						<ToolbarSpacer/>
						<Title level="Auto" text="Fortschritt"/>
						<ToolbarSpacer/>
						<Button width="15%" icon="sap-icon://customer" press="onDisplayProfile"/>
						<ToolbarSpacer width="5%"/>
					</Toolbar>
					<content></content>
					<footer>
						<OverflowToolbar>
							<ToolbarSpacer width="5%"/>
							<Button width="15%" icon="sap-icon://competitor"/>
							<ToolbarSpacer/>
							<Button width="15%" icon="sap-icon://physical-activity" press="onDisplayTraining"/>
							<ToolbarSpacer/>
							<Button width="15%" icon="sap-icon://electronic-medical-record" press="onDisplayMedicalFile"/>
							<ToolbarSpacer width="5%"/>
						</OverflowToolbar>
					</footer>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>