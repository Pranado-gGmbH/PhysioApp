{ 
    "welcomeFile": "index.html", 
    "authenticationMethod": "route",
	"routes": [
		{
			"source": "^/api/(.*)$",
			"target": "$1",
			"destination": "physio-backend"
		},
		{
			"source" : "^/media/(.*)$",
			"target" : "$1",
			"destination" : "media-share"
		},
		{ 
			"source": "^/(.*)", 
			"localDir": "webapp" 
		} 
	] 
}