<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="sapcp.cf.strongpeople.physioapp.controller.PatientsMasterList" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>pageTitlePatientsMasterList}">
                <subHeader>
                    <OverflowToolbar>
                        <SearchField
                            liveChange="onPatientSearch"
                            width="100%" />
                    </OverflowToolbar>
                </subHeader>
				<content>
					<List id="patientsList" items="{Patients>/patients}">
                        <items>
					        <StandardListItem
                                id="patientListItemId"
                                title="{Patients>Vname}"
                                description="{Patients>email}"
                                icon="{Patients>url_image}"
                                iconDensityAware="false"
                                iconInset="false"
                                type="Navigation"
                                press="onPatientPressMasterPage"/>
                        </items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>