{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sapcp.cf.strongpeople.physioapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sapcp.cf.strongpeople.physioapp.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapcp.cf.strongpeople.physioapp.i18n.i18n"
				}
			},
			"Videos": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/Videos.json"
            },
            "Patients": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/Patients.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sapcp.cf.strongpeople.physioapp.view",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "default",
					"pattern": "",
					"target": [
						"patientsMasterList",
						"welcome"
					]
                },
                {
					"name": "patientsMasterList",
					"pattern": "patients/{patIndex}",
					"target": [
                        "patientDetailPage",
                        "patientsMasterList"
					]
				},
				{
					"name": "patientVideos",
					"pattern": "patients/{patIndex}/videos",
					"target": [
						"patientsMasterList",
						"patientVideos"
					]
				}
			],
			"targets": {
                "patientsMasterList": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"controlAggregation": "masterPages",
					"viewId": "PatientsMasterList",
					"viewName": "PatientsMasterList"
                },
                "patientDetailPage": {
                    "viewType": "XML",
					"viewLevel": 1,
					"viewName": "PatientDetailPage",
					"controlAggregation": "detailPages"
                },
				"welcome": {
					"viewType": "XML",
					"viewLevel": 0,
					"viewName": "Welcome",
					"controlAggregation": "detailPages"
				},
				"patientVideos": {
					"viewType": "XML",
					"viewLevel": 2,
					"viewName": "PatientVideos",
					"controlAggregation": "detailPages"
				}
			}
		}
	}
}